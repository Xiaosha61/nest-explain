{
    "name": "nest-explain",
    "version": "1.0.0",
    "description": "nest-js playground, confusion explaining",
    "main": "index.js",
    "scripts": {
        "initProject": "nest new demo",
        "prebuild": "rm -rf dist",
        "build": "nest build",
        "start:multi-providers": "APP_PATH=multi-providers-in-one-module nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Xiaosha61/nest-explain.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Xiaosha61/nest-explain/issues"
    },
    "homepage": "https://github.com/Xiaosha61/nest-explain#readme",
    "dependencies": {
        "@nestjs/cli": "^8.1.5",
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/platform-express": "^8.0.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.6.0",
        "eslint": "^8.4.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.5.1"
    }
}
